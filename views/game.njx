{% extends "./templates/layout.njx" %}

{% block content %}
  
{% include "./templates/partials/header.njx" %}

  

  <div id="container">
    <div id="board">
      {{error}}
      {% for image in userImages %}
        <div class="images">
          <div class="overlay"></div>
          <img src="{{ image.images.thumbnail.url}}" data-key="{{loop.index}}" data-image="{{image.id}}">
        </div>
      {% endfor %}
    </div>
  </div>
  
{% endblock %}